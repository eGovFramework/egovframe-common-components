<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE form-validation PUBLIC 
    "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1//EN" 
    "http://jakarta.apache.org/commons/dtds/validator_1_1.dtd">

<form-validation>

    <formset>
		<form name="history">
			<field property="sysNm" depends="required, maxlength">
        		<arg0 key="comSymLogSlg.sysHistManage.validate.sysNm" />
        		<arg1 key="120" resource="true"/>
				<var>
					<var-name>maxlength</var-name>
					<var-value>120</var-value>
				</var>
			</field>
			
			<field property="histCn" depends="required, maxlength">
        		<arg0 key="comSymLogSlg.sysHistManage.validate.histCn" />
        		<arg1 key="2000" resource="true"/>
				<var>
					<var-name>maxlength</var-name>
					<var-value>2000</var-value>
				</var>
			</field> 
			<field property="histSeCode" depends="required">
        		<arg0 key="comSymLogSlg.sysHistManage.validate.histSeCode" />
			</field>			
					
		</form>
    </formset>

</form-validation>